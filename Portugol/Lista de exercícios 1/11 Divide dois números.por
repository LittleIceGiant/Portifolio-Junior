//Crie um programa que dívida dois números digitados pelo usuário.
algorithm divideDoisNumeros;
var
    double: num1, num2, resultado;
start
    input("Escreva aqui o primeiro número: ")
    read(num1)
    input("Escreva aqui o segundo número: ")
    read(num2)
    resultado <- num1 / num2
end
end_algorithm
