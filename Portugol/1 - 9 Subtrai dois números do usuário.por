//Crie um programa que subtraia dois números digitados pelo usuário.
algorithm subtraiDoisNumeros;
var
    float: num1, num2, resultado1
start
    input("Escreva o primeiro número da operação: ")
    read(num1)
    input("Escreva o segundo número da operação: ")
    read(num2)
    resultado1 <- num1 - num2
end
end_algorithm
